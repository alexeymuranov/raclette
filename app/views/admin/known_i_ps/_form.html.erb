<%= form_for(@known_ip) do |f| %>

  <%= render 'validation_error_messages' %>

  <%= field_set_tag do %>
    <dl class="form_fields">
      <% [:ip, :description ].each do |attribute| %>

        <%= render 'shared/attribute_in_form',
                   :form_builder => f,
                   :model        => @known_ip,
                   :attribute    => attribute %>


      <% end %>
    </dl>
  <% end %>

  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
