<tr>
  <th scope=row>
    <%= link_to known_ip.public_send(displayed_columns[0]), known_ip,
                :title => t('actions.read'),
                :class => 'display' %>
  </th>

  <% displayed_columns[1..-1].each do |attribute| %>
    <td><%= known_ip.public_send(attribute) %></td>
  <% end %>
</tr>
