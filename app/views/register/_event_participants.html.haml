%section#participants
  %h2= t 'register.choose_person.subheaders.participants'
  %ol
    - @participants.each do |p|
      %li= p.virtual_full_name

  %span.actions
    %button{ :name  => 'button',
             :type  => 'submit',
             :value => 'hide_participants' }
      = t('buttons.hide_participants')
